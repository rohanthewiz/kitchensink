{
  "name": "kitchensink",
  "version": "1.0.0",
  "description": "Demonstration of React with a few controls, running on a Go backend",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile",
    "compile": "browserify src/index.jsx -t babelify -o dist/js/bundle.js",
    "watch": "onchange 'src/**/*.jsx' -- npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Rohan Allison",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "http-server": "^0.9.0",
    "onchange": "^3.0.2"
  }
}
